<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xareni.proyectobasedatos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegistrarUsuario"
            android:label="@string/title_activity_registrar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ConsultarUsuario"
            android:label="@string/title_activity_consultar_usuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EliminarUsuario"
            android:label="@string/title_activity_eliminar_usuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ModificarUsuario"
            android:label="@string/title_activity_modificar_usuario"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
